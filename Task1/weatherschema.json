/*schema describing the structure of other data*/
{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1620979734.json", 
	"title": "Root", 
	"type": "array",
	"default": [],
	"items":{
		"$id": "#root/items", 
		"title": "Items", 
		"type": "object",
		"required": [ //required data
			"ID",
			"Name",
			"CurrentConditions",
			"Temperature",
			"WindSpeed",
			"WindDirection",
			"WindChillFactor"
		],
		"properties": {
			"ID": {
				"$id": "#root/items/ID",  // ID structure
				"title": "Id", 
				"type": "string",
				"default": "",
				"examples": [
					"1"
				],
				"pattern": "^.*$"
			},
			"Name": {
				"$id": "#root/items/Name", //name structure
				"title": "Name", 
				"type": "string",
				"default": "",
				"examples": [
					"Bath"
				],
				"pattern": "^.*$"
			},
			"CurrentConditions": {
				"$id": "#root/items/CurrentConditions", //currectcondition structure
				"title": "Currentconditions", 
				"type": "string",
				"default": "",
				"examples": [
					"Clouds"
				],
				"pattern": "^.*$"
			},
			"Temperature": {
				"$id": "#root/items/Temperature", //temperature structure
				"title": "Temperature", 
				"type": "string",
				"default": "",
				"examples": [
					"12.5"
				],
				"pattern": "^.*$"
			},
			"WindSpeed": {
				"$id": "#root/items/WindSpeed", // windspeed structure
				"title": "Windspeed", 
				"type": "string",
				"default": "",
				"examples": [
					"1.98"
				],
				"pattern": "^.*$"
			},
			"WindDirection": {
				"$id": "#root/items/WindDirection",  //wind direction structure
				"title": "Winddirection", 
				"type": "string",
				"default": "",
				"examples": [
					"87"
				],
				"pattern": "^.*$"
			},
			"WindChillFactor": {
				"$id": "#root/items/WindChillFactor",  //windchillfactor structure
				"title": "Windchillfactor", 
				"type": "string",
				"default": "",
				"examples": [
					"35"
				],
				"pattern": "^.*$"
			}
		}
	}

}